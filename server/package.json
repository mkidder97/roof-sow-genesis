{
  "name": "roof-sow-backend",
  "version": "10.0.0",
  "description": "Enhanced SOW Generator Backend with COMPLETE Real-Time Collaboration, Live WebSocket Integration, Push Notifications, Activity Feeds, Status Synchronization, Multi-Role Workflow, File Management, Section Engine & Self-Healing",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch index.ts",
    "dev:enhanced": "tsx watch index-enhanced.ts",
    "dev:professional": "tsx watch index-professional.ts",
    "dev:supabase": "tsx watch index-supabase.ts",
    "dev:realtime": "tsx watch index-realtime-integrated.ts",
    "start": "tsx index-realtime-integrated.ts",
    "start:enhanced": "tsx index-enhanced.ts",
    "start:professional": "tsx index-professional.ts",
    "start:supabase": "tsx index-supabase.ts",
    "start:realtime": "tsx index-realtime-integrated.ts",
    "build": "tsc",
    "start:production": "node dist/index-realtime-integrated.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scrape:test": "tsx scrapers/manufacturer-scraping-service.js",
    "scrape:carlisle": "tsx scrapers/carlisle-real-scraper.js",
    "scrape:approvals": "tsx scrapers/automated-approvals-service.js",
    "files:cleanup": "tsx core/file-management.js --cleanup",
    "files:stats": "tsx core/file-management.js --stats",
    "realtime:test": "tsx core/realtime-server.js --test",
    "notifications:test": "tsx core/notification-system.js --test",
    "collaboration:test": "tsx routes/realtime-collaboration.js --test"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "pdfkit": "^0.13.0",
    "@supabase/supabase-js": "^2.39.3",
    "puppeteer": "^22.15.0",
    "cheerio": "^1.0.0-rc.12",
    "sharp": "^0.33.2",
    "exifreader": "^4.21.0",
    "uuid": "^9.0.1",
    "mime-types": "^2.1.35",
    "file-type": "^19.0.0",
    "socket.io": "^4.7.5",
    "nodemailer": "^6.9.8",
    "@types/nodemailer": "^6.4.14",
    "jsonwebtoken": "^9.0.2",
    "rate-limiter-flexible": "^4.0.1",
    "joi": "^17.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.5",
    "@types/pdfkit": "^0.12.9",
    "@types/uuid": "^9.0.7",
    "@types/mime-types": "^2.1.4",
    "@types/jsonwebtoken": "^9.0.5",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
