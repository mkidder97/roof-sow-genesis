{
  "name": "roof-sow-backend",
  "version": "12.0.0-unified",
  "description": "Unified Roof SOW Genesis Backend - Complete Integration with Enhanced Intelligence, Manufacturer Analysis, Wind Calculations, and Frontend Connection",
  "main": "dist/index-unified.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch index-unified.ts",
    "dev:legacy": "tsx watch index.ts",
    "start": "tsx index-unified.ts",
    "start:legacy": "tsx index.ts",
    "build": "tsc index-unified.ts --outDir dist --target es2020 --module es2020 --moduleResolution node",
    "start:production": "node dist/index-unified.js",
    "test": "npm run test:integration",
    "test:integration": "tsx test-unified-integration.ts",
    "test:enhanced-intelligence": "tsx test-enhanced-intelligence.ts",
    "test:manufacturer-analysis": "tsx test-manufacturer-analysis.ts",
    "scrape:test": "tsx scrapers/manufacturer-scraping-service.js",
    "scrape:carlisle": "tsx scrapers/carlisle-real-scraper.js",
    "scrape:approvals": "tsx scrapers/automated-approvals-service.js",
    "files:cleanup": "tsx core/file-management.js --cleanup",
    "files:stats": "tsx core/file-management.js --stats",
    "test:pdf": "tsx core/takeoff-engine.ts --test-pdf",
    "test:ocr": "tsx core/takeoff-engine.ts --test-ocr",
    "test:sow-api": "tsx routes/sow-generation-api.ts --test",
    "test:enhanced-intelligence": "tsx routes/enhanced-intelligence.ts --test"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "pdfkit": "^0.13.0",
    "@supabase/supabase-js": "^2.39.3",
    "puppeteer": "^22.15.0",
    "cheerio": "^1.0.0-rc.12",
    "sharp": "^0.33.2",
    "exifreader": "^4.21.0",
    "uuid": "^9.0.1",
    "mime-types": "^2.1.35",
    "file-type": "^19.0.0",
    "socket.io": "^4.7.5",
    "nodemailer": "^6.9.8",
    "@types/nodemailer": "^6.4.14",
    "jsonwebtoken": "^9.0.2",
    "rate-limiter-flexible": "^4.0.1",
    "joi": "^17.11.0",
    "pdf-parse": "^1.1.1",
    "tesseract.js": "^5.0.4",
    "xlsx": "^0.18.5",
    "papaparse": "^5.4.1",
    "canvas": "^2.11.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.5",
    "@types/pdfkit": "^0.12.9",
    "@types/uuid": "^9.0.7",
    "@types/mime-types": "^2.1.4",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/papaparse": "^5.3.14",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
